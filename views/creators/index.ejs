<h2 class="page-header">Search Creators</h2>
<form action="/creators" method="GET"> 
    <label>Name</label>
    <input type="text" name="name" value="<%= searchOptions.name %>">
    <button type="submit">Search</button>
</form> 
<br>
<% creators.forEach(creator => { %>
    <div><%= creator.name %></div>
    <div class="btn-row">
    <a class="btn btn-primary" href="/creators/<%= creator.id %>">View</a>
    <a class="btn btn-primary" href="/creators/<%= creator.id %>/edit">Edit</a>
    <%- include('../partials/deleteForm', { url: `/creators/${creator.id}` }) %>
</div>        

<% }) %>

    